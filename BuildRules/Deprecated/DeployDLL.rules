<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="Deploy DLL"
	Version="8.00"
	>
	<Rules>
		<CustomBuildRule
			Name="Packages"
			DisplayName="Package Deploy"
			CommandLine="copy /Y &quot;$(InputPath)&quot; $(YAGET_RUN_FOLDER)\"
			Outputs="$(YAGET_RUN_FOLDER)\$(InputFileName)"
			FileExtensions="*.dll;*.manifest"
			ExecutionDescription="coping $(InputPath) to $(YAGET_RUN_FOLDER)"
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
