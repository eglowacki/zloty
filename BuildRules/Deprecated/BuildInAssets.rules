<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="BuildIn Assets"
	Version="8.00"
	>
	<Rules>
		<CustomBuildRule
			Name="BuildIn Assets"
			DisplayName="BuildIn Assets"
			CommandLine="$(YAGET_ROOT_FOLDER)\Tools\sfk\sfk.exe bin-to-src -hex $(InputPath) $(YAGET_ROOT_FOLDER)\branch\$(YAGET_BRANCH_FOLDER)\generated\$(ProjectName)\$(InputName).h $(InputName)"
			Outputs="$(YAGET_ROOT_FOLDER)\branch\$(YAGET_BRANCH_FOLDER)\generated\$(ProjectName)\$(InputName).h"
			FileExtensions="*.x;*.fx;*.mat;*.tga"
			ExecutionDescription="Generated $(YAGET_ROOT_FOLDER)\branch\$(YAGET_BRANCH_FOLDER)\generated\$(ProjectName)\$(InputName).h from $(InputPath)"
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
